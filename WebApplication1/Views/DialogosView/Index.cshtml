



@model IEnumerable<Domain.Dialogo>

@{
    ViewBag.Title = "Conversaciones";
}

<h2>Conversaciones</h2>

<table class="table">
    <tr>
        <th>
            Contacto
        </th>
        <th>
           
        </th>    
    </tr>

    @foreach (var item in Model)
    {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Cliente.ChatId)
    </td>
    

    <td>
        @Html.ActionLink("Ir a resolver", "Details/" + item.ID)
        
    </td>
</tr>
    }

</table>


